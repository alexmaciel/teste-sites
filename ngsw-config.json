{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/icons/*.png",
          "/assets/svg/*.svg",
          "/assets/images/*.svg",
          "/assets/images/*.png",
          "/assets/images/*.jpg",          
          "/assets/images/*.webp",          
          "/manifest.webmanifest",
          "/favicon.ico",
          "/index.html",          
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://fonts.googleapis.com/css2?family=Krub:ital,wght@0,400;0,500;0,700;1,400;1,500&display=swap",
          "https://fonts.googleapis.com/**"
        ]        
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/media/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-fresh",
      "urls": [
        "https://metabixbiotech.com/api/clients/slides**",
        "https://metabixbiotech.com/api/clients/posts**"
      ],
      "cacheQueryOptions": { "ignoreSearch": false },
      "cacheConfig": {
        "strategy": "freshness",
        "timeout": "0u",
        "maxSize": 100,
        "maxAge": "30m"
      }
    },
    {
      "name": "api-static",
      "urls": [
        "https://metabixbiotech.com/api/clients/categories**",
        "https://metabixbiotech.com/api/clients/company**",
        "https://metabixbiotech.com/api/clients/companyItems**",
        "https://metabixbiotech.com/api/clients/companyGoals**",
        "https://metabixbiotech.com/api/clients/technology**",
        "https://metabixbiotech.com/api/clients/technologyItems**",
        "https://metabixbiotech.com/api/clients/social**",
        "https://metabixbiotech.com/api/clients/settings**"
      ],
      "cacheQueryOptions": { "ignoreSearch": false },
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 200,
        "maxAge": "1d"
      }
    }           
  ]    
}
